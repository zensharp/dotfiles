{
    "autoclose": false,
    "clipboard": "terminal",
    "colorscheme": "minimal",
    "eofnewline": false,
    "indentchar": "→",
    "matchbrace": false,
    "mkparents": true,
    "scrollbar": true,
    "scrollspeed": 1,
    "softwrap": true,
    "statusformatl": "$(filename) $(modified)",
    "statusformatr": "Line $(line), Column $(col) • $(opt:filetype)",
    "useprimary": false
}
