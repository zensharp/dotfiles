{
    "csharp.referencesCodeLens.enabled": false,
    "diffEditor.codeLens": true,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.fontFamily": "Iosevka SS16 SemiBold, Cascadia Code PL, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.formatOnType": true,
    "editor.insertSpaces": false,
    "editor.renderWhitespace": "all",
    "editor.smoothScrolling": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.decorations.badges": false,
    "explorer.expandSingleFolderWorkspaces": false,
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "off",
    "files.hotExit": "off",
    "gitpod.openInStable.neverPrompt": true,
    "omnisharp.useModernNet": true,
    "omnisharp.monoPath": "C:/Program Files/Mono/bin/mono.exe",
    "omnisharp.useGlobalMono": "always",
    "problems.decorations.enabled": false,
    "redhat.telemetry.enabled": false,
    "search.useGlobalIgnoreFiles": true,
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "window.restoreWindows": "none",
    "workbench.colorCustomizations": {
        "terminal.ansiBrightMagenta": "#FF8000",
        "terminal.ansiMagenta": "#FF8000",
        "editorSuggestWidget.background": "#141414e0",
        "editorWidget.background": "#141414e0",
        "editorHoverWidget.background": "#141414e0",
        "[Nord]": {
            "editor.background": "#272C36",
            "editorGutter.background": "#272C36",
            "editor.lineHighlightBackground": "#3b425240",
            "editor.lineHighlightBorder": "#3b425280",
            "editor.selectionBackground": "#5e81ac80",
            "editor.selectionHighlightBackground": "#8fbcbb18",
            "activityBar.background": "#3b4252",
            "activityBar.activeBackground": "#4c566a",
            "titleBar.activeBackground": "#3b4252",
            "titleBar.inactiveBackground": "#3b4252"
        }
    },
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.decorations.badges": false,
    "workbench.editor.decorations.colors": false,
    "workbench.editor.openPositioning": "last",
    "workbench.editor.untitled.hint": "hidden",
    "workbench.enableExperiments": false,
    "workbench.list.smoothScrolling": true,
    "workbench.startupEditor": "newUntitledFile",
    "editor.tabSize": 3,
    "editor.fontSize": 16,
    "workbench.colorTheme": "Nord",
    "editor.wordWrap": "on",
    "editor.minimap.renderCharacters": false,
    "editor.minimap.enabled": false,
    "window.zoomLevel": 2,
    
}
