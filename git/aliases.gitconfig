[alias]
	st = status
	s = status --short
	ft = fetch
	pl = pull
	ps = push
	co = checkout
	cob = checkout -b
	sw = switch
	swc = switch -C
	aa = add --all
	cm = commit
	cma = commit -a
	amend = commit --amend
	adog = log --all --decorate --oneline --graph
	root = rev-parse --show-toplevel
	pd = produce
	bw = browse
	lz = lazy
	pb = publish
	pbc = publish --create
	pbca = publish --create --merge-when-pipeline-succeeds
