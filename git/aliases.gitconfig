[alias]
	st = status
	s = status --short
	cl = clone
	ft = fetch
	pl = pull
	ps = push
	rb = rebase
	mg = merge
	br = branch
	co = checkout
	cob = checkout -b
	si = switch
	sic = switch -C
	aa = add --all
