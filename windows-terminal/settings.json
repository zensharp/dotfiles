{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": 
            {
                "action": "globalSummon",
                "desktop": "toCurrent",
                "dropdownDuration": 1,
                "monitor": "toMouse",
                "name": "_quake",
                "toggleVisibility": true
            },
            "keys": "win+`"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "shift+up"
        },
        {
            "command": 
            {
                "action": "newTab"
            },
            "keys": "ctrl+t"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "shift+down"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "shift+left"
        },
        {
            "command": 
            {
                "action": "clearBuffer",
                "clear": "all"
            },
            "keys": "ctrl+k"
        },
        {
            "command": 
            {
                "action": "closeTab"
            },
            "keys": "ctrl+w"
        },
        {
            "command": "quit",
            "keys": "ctrl+shift+q"
        },
        {
            "command": "closeWindow"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "previous"
            }
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "shift+right"
        },
        {
            "command": 
            {
                "action": "sendInput",
                "input": "\u001b["
            },
            "keys": "ctrl+shift+enter"
        }
    ],
    "alwaysShowNotificationIcon": true,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{2c4de342-38b7-51cf-b940-2309a097f518}",
